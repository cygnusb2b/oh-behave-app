{{#link-to "property.queries.view" model.id class="list-group-item list-group-item-action flex-column align-items-start"}}
  <div class="d-flex w-100 justify-content-between">
    <h5 class="mb-1">{{model.name}}</h5>
    <div class="text-right">
      {{#if model.updatedAt}}
        <small class="d-block" title="{{moment-format model.updatedAt 'MMM Do, YYYY h:mm a'}}">
          {{entypo-icon "clock"}} Updated {{moment-from-now model.updatedAt}}
        </small>
      {{/if}}
      <small class="d-block" title="{{model.createdBy.email}}">
        {{entypo-icon "user"}} Created by {{model.createdBy.givenName}} {{model.createdBy.familyName}}
      </small>
    </div>
  </div>

  {{!-- <p class="mb-1">Base {{model.baseVersion}} ({{model.userSource}})</p>
  <small class="d-block">{{model.key}}</small> --}}
  <small class="d-block">ID: {{model.id}}</small>
{{/link-to}}
